{
  "name": "@cotter45/tsfw",
  "description": "Unopinionated React like framework with data binding, type safe routing, nested layouts, plus built in local, global, and persistant state management.",
  "version": "0.0.13",
  "author": "Sean Cotter <sd.cotter45@gmail.com>",
  "license": "ISC",
  "keywords": [],
  "main": "bin/tsfw.cjs",
  "type": "module",
  "bin": {
    "create-tsfw": "./bin/tsfw.cjs"
  },
  "files": [
    "bin",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "check": "npm run lint && npm run format && npm run test:coverage",
    "lint": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "preview": "vite preview",
    "serve": "npx serve@latest dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "update": "ncu -u && npm install"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.10.2",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitest/browser": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.22",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^25.0.1",
    "npm-check-updates": "^17.1.11",
    "postcss": "^8.4.49",
    "prismjs": "^1.29.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.8"
  }
}
